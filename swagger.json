  "paths": {
    "/iconify/{token}": {
      "post": {
        "operationId": "postMessage",
        "description": "Post a new messages to a stall",
        "parameters": [
          {
            "name": "token",
            "in": "url",
            "description": "Token that represents a public key",
            "required": true,
            "example": "55bad0ba6562330001000001",
            "type": "string"
          }
        ]
      }
    }
  }
